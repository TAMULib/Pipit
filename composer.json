{
    "name": "tamu-lib/pipit",
    "type": "library",
    "description": "A basic PHP framework for web app development",
    "keywords": ["MVC","Framework"],
    "homepage": "https://github.com/TAMULib/Pipit",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Savell",
            "email": "jsavell@library.tamu.edu",
            "homepage": "https://github.com/jsavell",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "TestFiles\\": "tests/TestFiles/"
        }
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "phpstan/phpstan": "^1.5"
    }
}
